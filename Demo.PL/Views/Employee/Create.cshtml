@model EmployeeViewModel;
@{
    ViewData["Title"] = "Create";
}


<h1>Create New Employee</h1>
<form asp-action="Create" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Image" class="col-form-label"></label>
        <input asp-for="Image" class="form-control-file" />
    </div>
    <partial name="PartialViews/UpdateCreateEmployeePartailView"></partial>

    <input type="submit" value="Create" class="btn btn-primary" />
    <a asp-controller="Department" asp-action="Index" class="btn btn-light">Back To List</a>
</form>
@section validation
{
<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}

